## 10 nested lamdas with same var

fun get_nested_lambda() do
    fun (a1) do
        fun (a2) do
            fun (a3) do
                fun (a4) do
                    fun (a5) do
                        fun (a6) do
                            fun (a7) do
                                fun (a8) do
                                    fun (a9) do
                                        fun (a10) do
                                            a1 ++ ' ' ++ a2 ++ ' ' ++ a3 ++ ' ' ++ a4 ++ ' ' ++ a5 ++ ' ' ++ a6 ++ ' ' ++ a7 ++ ' ' ++ a8 ++ ' ' ++ a9 ++ ' ' ++ a10
                                        end
                                    end
                                end
                            end
                        end
                    end
                end
            end
        end
    end
end

res = get_nested_lambda()
res = res('Nested')
res = res('Lambdas')
res = res('in')
res = res('a')
res = res('chain')
res = res('of')
res = res('functions')
res = res('that')
res = res('return')
res = res('!!')

assert(res == 'Nested Lambdas in a chain of functions that return !!', 'fail to mount nested lamdas!')

