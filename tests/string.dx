fun say(name) do
    name
end

fun say(name, last) do
    name ++ " " ++ last
end

assert(say("one arg") == "one arg", "os error: say one arg")

expected = (say("Hello", "World"))

assert(expected == "Hello World", "os error: Concat with nested functions")

